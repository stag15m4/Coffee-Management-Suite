# Development Mode - Use mock data for testing without database
VITE_USE_MOCK_DATA=true

# Supabase Configuration (dummy values for dev mode - not used when VITE_USE_MOCK_DATA=true)
VITE_SUPABASE_URL=https://placeholder.supabase.co
VITE_SUPABASE_ANON_KEY=placeholder-key-not-used-in-dev-mode

