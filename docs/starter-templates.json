    "equipment": [
      {
        "name": "Espresso Machine",
        "category": "Brewing",
        "typical_maintenance": [
          {
            "task": "Backflush with cleaner",
            "interval_days": 1,
            "description": "Run backflush cycle with espresso machine cleaner (e.g., Cafiza) to remove coffee oil buildup from the group heads."
          },
          {
            "task": "Descale boiler",
            "interval_days": 30,
            "description": "Run descaling solution through the boiler and steam system to remove mineral deposits. Follow manufacturer instructions."
          },
          {
            "task": "Replace group head gaskets",
            "interval_days": 365,
            "description": "Inspect and replace rubber group head gaskets and shower screens. Worn gaskets cause leaks and poor extraction."
          },
          {
            "task": "Professional service",
            "interval_days": 180,
            "description": "Schedule professional technician for full inspection: pressure calibration, boiler inspection, valve checks, and seal replacement."
          }
        ]
      },
      {
        "name": "Coffee Grinder",
        "category": "Brewing",
        "typical_maintenance": [
          {
            "task": "Calibrate grind size",
            "interval_days": 7,
            "description": "Dial in espresso grind by pulling test shots. Adjust for changes in bean age, humidity, and roast variation."
          },
          {
            "task": "Clean burrs and chamber",
            "interval_days": 7,
            "description": "Remove hopper and upper burr. Brush out retained grounds and coffee oils from burr chamber. Use grinder cleaning tablets monthly."
          },
          {
            "task": "Replace burrs",
            "interval_days": 365,
            "description": "Inspect burr sharpness and replace if dull or chipped. Flat burrs typically last 600-800 lbs of coffee; conical burrs last longer."
          }
        ]
      },
      {
        "name": "Batch Brewer",
        "category": "Brewing",
        "typical_maintenance": [
          {
            "task": "Clean brew basket and carafe",
            "interval_days": 1,
            "description": "Wash brew basket, spray head, and carafe with hot soapy water. Remove coffee oil residue that causes bitter flavors."
          },
          {
            "task": "Descale",
            "interval_days": 30,
            "description": "Run descaling solution through the brewing system to remove calcium and mineral buildup from water lines and heating element."
          },
          {
            "task": "Replace spray head",
            "interval_days": 180,
            "description": "Inspect and replace the spray head if water distribution is uneven, which leads to under-extraction."
          }
        ]
      },
      {
        "name": "Blender",
        "category": "Brewing",
        "typical_maintenance": [
          {
            "task": "Clean and sanitize",
            "interval_days": 1,
            "description": "Blend hot soapy water after each use session. Deep clean jar, lid, and blade assembly daily."
          },
          {
            "task": "Inspect blade and gasket",
            "interval_days": 30,
            "description": "Check blade sharpness and rubber gasket seal. Replace if blending is inconsistent or jar leaks."
          }
        ]
      },
      {
        "name": "Commercial Refrigerator",
        "category": "Refrigeration",
        "typical_maintenance": [
          {
            "task": "Check and log temperature",
            "interval_days": 1,
            "description": "Verify internal temperature is 35-38F. Log reading for health code compliance. Adjust thermostat if needed."
          },
          {
            "task": "Clean condenser coils",
            "interval_days": 90,
            "description": "Vacuum or brush dust and debris from condenser coils (usually on the back or bottom). Dirty coils reduce efficiency and can cause failure."
          },
          {
            "task": "Inspect door gaskets",
            "interval_days": 90,
            "description": "Check rubber door gaskets for cracks, tears, or weak seal. Replace if the dollar-bill test fails (bill slides out when door is closed)."
          },
          {
            "task": "Deep clean interior",
            "interval_days": 30,
            "description": "Remove all items, wipe down shelves and walls with food-safe sanitizer. Check for expired items and clean drain pan."
          }
        ]
      },
      {
        "name": "Ice Machine",
        "category": "Refrigeration",
        "typical_maintenance": [
          {
            "task": "Clean and sanitize bin",
            "interval_days": 7,
            "description": "Wipe down ice bin interior with food-safe sanitizer. Check for mold or slime buildup especially around the door and chute."
          },
          {
            "task": "Run cleaning cycle",
            "interval_days": 90,
            "description": "Run manufacturer-recommended cleaning solution through the ice-making system. This removes scale and mineral buildup from evaporator plates."
          },
          {
            "task": "Replace water filter",
            "interval_days": 180,
            "description": "Replace the inline water filter cartridge. Clogged filters reduce ice production and can introduce off-flavors."
          },
          {
            "task": "Clean condenser",
            "interval_days": 90,
            "description": "Vacuum or brush condenser fins to remove dust. Ice machines in warm environments need this more frequently."
          }
        ]
      },
      {
        "name": "Dishwasher",
        "category": "Other",
        "typical_maintenance": [
          {
            "task": "Clean wash and rinse arms",
            "interval_days": 7,
            "description": "Remove wash and rinse arms and clear any debris from spray nozzles. Clogged nozzles reduce cleaning effectiveness."
          },
          {
            "task": "Check water temperature",
            "interval_days": 7,
            "description": "Verify wash temp reaches 150F minimum and final rinse reaches 180F (or per local health code). Adjust heating element if needed."
          },
          {
            "task": "Descale",
            "interval_days": 30,
            "description": "Run a descaling cycle or add descaling solution to remove mineral deposits from the tank, heating element, and spray arms."
          },
          {
            "task": "Clean drain and filter",
            "interval_days": 1,
            "description": "Remove and rinse the drain screen and filter basket. Food debris causes odors and reduces water flow."
          }
        ]
      },
      {
        "name": "POS System",
        "category": "Other",
        "typical_maintenance": [
          {
            "task": "Software update check",
            "interval_days": 30,
            "description": "Check for and install POS software updates. Updates often include security patches, tax table changes, and feature improvements."
          },
          {
            "task": "Clean touchscreen and card reader",
            "interval_days": 7,
            "description": "Wipe touchscreen with electronics-safe cleaner. Run a cleaning card through the chip/swipe reader to prevent read errors."
          },
          {
            "task": "Backup transaction data",
            "interval_days": 7,
            "description": "Verify automatic cloud backups are completing. Run a manual backup if needed. Check that end-of-day reports are archiving properly."
          },
          {
            "task": "Inspect cables and connections",
            "interval_days": 90,
            "description": "Check all power cables, ethernet, and peripheral connections (receipt printer, cash drawer, card reader). Replace worn cables."
          }
        ]
      }
    ],

