    "equipment": [
      {
        "name": "Pizza Oven (Deck)",
        "category": "Cooking",
        "typical_maintenance": [
          {
            "task": "Clean deck stones",
            "interval_days": 1,
            "description": "Brush flour, cornmeal, and charred debris off the deck stones with a brass or stainless steel brush after the oven cools. Never use water on hot stones."
          },
          {
            "task": "Check and calibrate temperature",
            "interval_days": 7,
            "description": "Verify oven temperature with an independent thermometer. Deck ovens should hold 500-550F for New York style or 700-800F for Neapolitan. Adjust thermostat if readings drift."
          },
          {
            "task": "Inspect door seals and hinges",
            "interval_days": 90,
            "description": "Check door gaskets for cracks and hinges for alignment. Poor seals cause heat loss and uneven baking."
          },
          {
            "task": "Professional inspection",
            "interval_days": 365,
            "description": "Schedule annual inspection by a commercial oven technician. Check gas lines, ignition system, thermostat calibration, and stone condition."
          }
        ]
      },
      {
        "name": "Dough Mixer (Spiral)",
        "category": "Dough Preparation",
        "typical_maintenance": [
          {
            "task": "Clean bowl, hook, and guards",
            "interval_days": 1,
            "description": "Remove dough hook and bowl. Wash with warm soapy water and sanitize. Wipe down the mixer body, safety guard, and splash ring."
          },
          {
            "task": "Grease gearbox and check belt",
            "interval_days": 90,
            "description": "Apply food-grade grease to gearbox fittings per manufacturer specs. Check drive belt tension and look for fraying or cracking."
          },
          {
            "task": "Inspect dough hook for wear",
            "interval_days": 180,
            "description": "Check the spiral hook for bending, cracks, or worn coating. A damaged hook affects dough development and can contaminate product."
          }
        ]
      },
      {
        "name": "Dough Sheeter / Roller",
        "category": "Dough Preparation",
        "typical_maintenance": [
          {
            "task": "Clean rollers and conveyor",
            "interval_days": 1,
            "description": "Wipe down rollers and conveyor belt with a damp cloth to remove dried dough. Do not submerge electrical components."
          },
          {
            "task": "Lubricate roller bearings",
            "interval_days": 30,
            "description": "Apply food-grade lubricant to roller bearings and pivot points. Dry bearings cause squealing and uneven sheet thickness."
          },
          {
            "task": "Check roller gap calibration",
            "interval_days": 30,
            "description": "Verify that both sides of the roller gap are even using a thickness gauge. Uneven gaps produce lopsided crusts."
          }
        ]
      },
      {
        "name": "Prep Table / Refrigerated Station",
        "category": "Preparation",
        "typical_maintenance": [
          {
            "task": "Clean and sanitize surface",
            "interval_days": 1,
            "description": "Wipe down the entire stainless steel surface with food-safe sanitizer. Clean all ingredient pan inserts and cutting board surfaces."
          },
          {
            "task": "Check refrigeration temperature",
            "interval_days": 1,
            "description": "Verify the cold rail holds ingredients at 40F or below. Log temperature for health code compliance."
          },
          {
            "task": "Clean condenser coils",
            "interval_days": 90,
            "description": "Vacuum condenser coils under or behind the unit. Dust buildup forces the compressor to work harder and can cause cooling failure."
          },
          {
            "task": "Inspect door gaskets",
            "interval_days": 90,
            "description": "Check rubber gaskets on access doors for cracks or loose fit. Replace if cold air is escaping."
          }
        ]
      },
      {
        "name": "Walk-in Cooler",
        "category": "Refrigeration",
        "typical_maintenance": [
          {
            "task": "Check and log temperature",
            "interval_days": 1,
            "description": "Verify internal temperature is 34-38F. Log reading for health code compliance. Investigate immediately if above 40F."
          },
          {
            "task": "Clean and organize interior",
            "interval_days": 7,
            "description": "Wipe down shelves and walls. Enforce FIFO (first in, first out) rotation. Check for expired items and clean any spills."
          },
          {
            "task": "Inspect evaporator coils and drain",
            "interval_days": 30,
            "description": "Check evaporator coils for ice buildup. Clear the condensate drain line to prevent water pooling on the floor."
          },
          {
            "task": "Check door gasket and hinges",
            "interval_days": 90,
            "description": "Inspect walk-in door gasket for tears or gaps. Check the door closer mechanism and hinges. A poor seal wastes energy and risks food safety."
          }
        ]
      },
      {
        "name": "Pizza Peel Set",
        "category": "Cooking",
        "typical_maintenance": [
          {
            "task": "Clean and dry peels",
            "interval_days": 1,
            "description": "Wipe wooden peels with a damp cloth (do not soak). Wash metal/aluminum peels with soapy water and dry completely to prevent rust."
          },
          {
            "task": "Sand and condition wooden peels",
            "interval_days": 30,
            "description": "Lightly sand wooden peels with fine-grit sandpaper to remove rough spots and charring. Apply food-grade mineral oil to prevent cracking."
          },
          {
            "task": "Inspect handles",
            "interval_days": 90,
            "description": "Check peel handles for cracks, looseness, or splinters. A broken handle near a 550F oven is a serious safety hazard."
          }
        ]
      },
      {
        "name": "Dough Docker",
        "category": "Dough Preparation",
        "typical_maintenance": [
          {
            "task": "Clean after use",
            "interval_days": 1,
            "description": "Wash dough docker pins and roller with warm soapy water. Remove any dried dough stuck between pins."
          },
          {
            "task": "Inspect pins and roller",
            "interval_days": 90,
            "description": "Check that all docker pins are intact and the roller spins freely. Missing or bent pins cause uneven docking and bubble formation during baking."
          }
        ]
      },
      {
        "name": "POS System",
        "category": "Other",
        "typical_maintenance": [
          {
            "task": "Software update check",
            "interval_days": 30,
            "description": "Check for and install POS software updates. Updates often include security patches, menu management improvements, and online ordering integration fixes."
          },
          {
            "task": "Clean touchscreen and peripherals",
            "interval_days": 7,
            "description": "Wipe touchscreen with electronics-safe cleaner. Clean card reader with a cleaning card. Wipe receipt printer housing and check paper supply."
          },
          {
            "task": "Backup data and verify reports",
            "interval_days": 7,
            "description": "Verify cloud backups are completing. Run a manual backup if needed. Check that daily sales summaries and inventory deductions are archiving properly."
          },
          {
            "task": "Inspect cables and connections",
            "interval_days": 90,
            "description": "Check all power cables, ethernet, kitchen display connections, and peripheral cables (printer, cash drawer, card reader). Replace worn cables."
          }
        ]
      }
    ],

